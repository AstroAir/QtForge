{
  "application": {
    "name": "QtForge Comprehensive Demo",
    "version": "3.0.0",
    "description": "Comprehensive demonstration of all QtForge features",
    "author": "QtForge Development Team",
    "license": "MIT"
  },
  "core": {
    "plugin_directory": "./plugins",
    "auto_load_plugins": true,
    "hot_reload_enabled": true,
    "dependency_resolution": true,
    "plugin_isolation": true,
    "max_plugins": 100,
    "load_timeout_ms": 30000
  },
  "communication": {
    "message_bus": {
      "enabled": true,
      "max_queue_size": 10000,
      "delivery_timeout_ms": 5000,
      "retry_attempts": 3,
      "compression_enabled": false
    },
    "request_response": {
      "enabled": true,
      "default_timeout_ms": 30000,
      "max_concurrent_requests": 1000,
      "request_queue_size": 5000
    }
  },
  "security": {
    "enabled": true,
    "level": "medium",
    "signature_verification": true,
    "trusted_publishers": ["com.qtforge", "org.qtforge", "com.example"],
    "sandbox_enabled": true,
    "permission_model": "whitelist",
    "audit_logging": true,
    "security_policies": {
      "allow_network_access": true,
      "allow_file_system_access": true,
      "allow_process_execution": false,
      "allow_registry_access": false
    }
  },
  "monitoring": {
    "enabled": true,
    "metrics_collection": {
      "enabled": true,
      "interval_ms": 5000,
      "history_size": 1000,
      "export_format": "json"
    },
    "health_monitoring": {
      "enabled": true,
      "check_interval_ms": 10000,
      "failure_threshold": 3,
      "recovery_timeout_ms": 60000
    },
    "performance_tracking": {
      "enabled": true,
      "track_command_execution": true,
      "track_message_processing": true,
      "track_memory_usage": true,
      "track_cpu_usage": true
    },
    "hot_reload": {
      "enabled": true,
      "watch_directories": ["./plugins"],
      "file_patterns": ["*.qtplugin", "*.json"],
      "debounce_ms": 1000
    }
  },
  "orchestration": {
    "enabled": true,
    "workflow_engine": {
      "max_concurrent_workflows": 50,
      "step_timeout_ms": 60000,
      "retry_failed_steps": true,
      "max_retries": 3
    },
    "scheduling": {
      "enabled": true,
      "max_scheduled_tasks": 100,
      "precision_ms": 100
    }
  },
  "transactions": {
    "enabled": true,
    "isolation_level": "read_committed",
    "timeout_ms": 30000,
    "max_concurrent_transactions": 100,
    "auto_rollback_on_error": true,
    "transaction_log": {
      "enabled": true,
      "log_file": "transactions.log",
      "max_log_size_mb": 100
    }
  },
  "marketplace": {
    "enabled": true,
    "repository_url": "https://marketplace.qtforge.org",
    "auto_update_check": true,
    "update_check_interval_hours": 24,
    "download_timeout_ms": 300000,
    "verify_signatures": true,
    "cache_directory": "./cache/marketplace"
  },
  "threading": {
    "thread_pool": {
      "min_threads": 2,
      "max_threads": 8,
      "idle_timeout_ms": 60000,
      "queue_size": 1000
    },
    "background_processing": {
      "enabled": true,
      "worker_threads": 4,
      "task_timeout_ms": 300000
    }
  },
  "python": {
    "enabled": false,
    "interpreter_path": "",
    "virtual_env_path": "",
    "module_search_paths": ["./python", "./python/modules"],
    "startup_scripts": ["python/init.py"],
    "max_execution_time_ms": 30000,
    "memory_limit_mb": 512
  },
  "logging": {
    "level": "info",
    "console_output": true,
    "file_output": {
      "enabled": true,
      "file_path": "comprehensive_demo.log",
      "max_file_size_mb": 50,
      "max_files": 5,
      "rotation": "daily"
    },
    "categories": {
      "qtforge.core": "debug",
      "qtforge.communication": "info",
      "qtforge.security": "info",
      "qtforge.monitoring": "info",
      "qtforge.plugin.comprehensive": "debug"
    }
  },
  "performance": {
    "optimization_level": "balanced",
    "memory_management": {
      "garbage_collection_interval_ms": 60000,
      "max_memory_usage_mb": 1024,
      "memory_pressure_threshold": 0.8
    },
    "caching": {
      "plugin_metadata_cache": true,
      "message_cache_size": 1000,
      "request_cache_size": 500
    }
  },
  "ui": {
    "enabled": false,
    "theme": "default",
    "window": {
      "width": 1200,
      "height": 800,
      "resizable": true,
      "center_on_screen": true
    },
    "features": {
      "plugin_manager_ui": true,
      "metrics_dashboard": true,
      "log_viewer": true,
      "configuration_editor": true
    }
  },
  "development": {
    "debug_mode": false,
    "verbose_logging": false,
    "profiling_enabled": false,
    "memory_debugging": false,
    "performance_profiling": false,
    "plugin_development_mode": false
  },
  "features": {
    "experimental": {
      "enabled": false,
      "features": [
        "advanced_caching",
        "distributed_plugins",
        "gpu_acceleration"
      ]
    },
    "deprecated": {
      "warn_on_usage": true,
      "features": []
    }
  },
  "limits": {
    "max_plugin_size_mb": 100,
    "max_message_size_kb": 1024,
    "max_request_size_kb": 1024,
    "max_response_size_kb": 1024,
    "max_log_entry_size_kb": 64
  },
  "timeouts": {
    "plugin_load_ms": 30000,
    "plugin_unload_ms": 10000,
    "command_execution_ms": 60000,
    "service_request_ms": 30000,
    "network_request_ms": 30000
  },
  "paths": {
    "plugins": "./plugins",
    "config": "./config",
    "logs": "./logs",
    "cache": "./cache",
    "temp": "./temp",
    "python": "./python"
  }
}
