{
  "name": "qtplugin",
  "version": "3.0.0",
  "description": "Modern C++ Plugin System for Qt Applications",
  "homepage": "https://github.com/QtForge/QtPlugin",
  "license": "MIT",
  "supports": "!uwp",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "qt6-base"
  ],
  "features": {
    "network": {
      "description": "Build network plugin support",
      "dependencies": [
        "qt6-networkauth"
      ]
    },
    "ui": {
      "description": "Build UI plugin support",
      "dependencies": [
        "qt6-tools"
      ]
    },
    "examples": {
      "description": "Build example plugins"
    }
  }
}
